{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Vala Project Schema",
  "required": [
    "name",
    "packages",
    "vapiFolders",
    "defines",
    "sourceFolder"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "myproj"
      ],
      "pattern": "^(.*)$"
    },
    "packages": {
      "$id": "#/properties/packages",
      "type": "array",
      "title": "The Packages Schema",
      "items": {
        "$id": "#/properties/packages/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          ""
        ],
        "pattern": "^(.*)$"
      }
    },
    "vapiFolders": {
      "$id": "#/properties/vapiFolders",
      "type": "array",
      "title": "The Vapifolders Schema",
      "items": {
        "$id": "#/properties/vapiFolders/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          ""
        ],
        "pattern": "^(.*)$"
      }
    },
    "defines": {
      "$id": "#/properties/defines",
      "type": "array",
      "title": "The Defines Schema",
      "items": {
        "$id": "#/properties/defines/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          ""
        ],
        "pattern": "^(.*)$"
      }
    },
    "sourceFolder": {
      "$id": "#/properties/sourceFolder",
      "type": "string",
      "title": "The Sourcefolder Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    }
  }
}
